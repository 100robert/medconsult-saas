    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest services/auth-service",
    "test:users": "jest services/users-service",
    "test:appointments": "jest services/appointments-service",
    "test:consultations": "jest services/consultations-service",
    "test:payments": "jest services/payments-service",
    "test:notifications": "jest services/notifications-service",
    "test:reviews": "jest services/reviews-service",
    "test:audit": "jest services/audit-service"
