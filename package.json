    "dev": "concurrently -n \"gateway,auth,users,appointments,consultations,payments,notifications,reviews,audit\" -c \"cyan,green,blue,yellow,magenta,red,white,gray,orange\" \"npm run dev:gateway\" \"npm run dev:auth\" \"npm run dev:users\" \"npm run dev:appointments\" \"npm run dev:consultations\" \"npm run dev:payments\" \"npm run dev:notifications\" \"npm run dev:reviews\" \"npm run dev:audit\"",
    "dev:gateway": "cd gateway && npm run dev",
    "dev:auth": "cd services/auth-service && npm run dev",
    "dev:users": "cd services/users-service && npm run dev",
    "dev:appointments": "cd services/appointments-service && npm run dev",
    "dev:consultations": "cd services/consultations-service && npm run dev",
    "dev:payments": "cd services/payments-service && npm run dev",
    "dev:notifications": "cd services/notifications-service && npm run dev",
    "dev:reviews": "cd services/reviews-service && npm run dev",
    "dev:audit": "cd services/audit-service && npm run dev",
    "dev:backend": "concurrently -n \"gateway,auth\" -c \"cyan,green\" \"npm run dev:gateway\" \"npm run dev:auth\"",
